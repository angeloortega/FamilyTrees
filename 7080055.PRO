	menda(Casado1,Casado2,Dia,Mes,Anho):- par(Casado1,Casado2,Dia,Mes,Anho).
	menda(Casado1,Casado2,Dia,Mes,Anho):- par(Casado2,Casado1,Dia,Mes,Anho).
	mendaAgagua(Casado1,Casado2):- menda(Casado1,Casado2,_,_,_),not(mendajueja(Casado1,Casado2,_,_,_)).
	mendaMboyve(Casado1,Casado2,Dia,Mes,Anho):- menda(Casado1,Casado2,Dia1,Mes1,Anho1),not(mendajuejaMboyve(Casado1,Casado2,Dia,Mes,Anho)),
