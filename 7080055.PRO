	taitaoSyreheguao(Taitao,Temiariroo):- tuva(Taitao, Sy), sy(Sy,Temiariroo).
	taitaotuvareheguao(Taitao,Temiariroo):- tuva(Taitao, Tuva), tuva(Tuva,Temiariroo).
	taitaxSyreheguax(Aaitax,Temiariroo):- monhahare(Aaitax, Sy), sy(Sy,Temiariroo).
	taitaxtuvareheguax(Aaitax,Temiariroo):- monhahare(Aaitax, Sy), tuva(Sy,Temiariroo).
	taitaaSyreheguaa(Taitaa,Temiariroo):- sy(Taitaa, Sy), sy(Sy,Temiariroo).
	taitaatuvareheguaa(Taitaa,Temiariroo):- sy(Taitaa, Tuva), tuva(Tuva,Temiariroo).
	taitao(Taitao,Temiariroo):- tuva(Taitao,Monhahare), monhahare(Monhahare,Temiariroo).
	taitaa(Taitaa,Temiariroo):- sy(Taitaa,Monhahare), monhahare(Monhahare,Temiariroo).
	taitax(Aaitax,Temiariroo) :- monhahare(Aaitax,Monhahare), monhahare(Monhahare,Temiariroo).

	temiariroo(Aaitax,Temiariroo) :- membyx(Aaitax,Monhahare), membyo(Monhahare,Temiariroo).
	temiariroa(Aaitax,Temiariroa) :- membyx(Aaitax,Monhahare), membya(Monhahare,Temiariroa).
	temiarirox(Aaitax,Temiarirox) :- membyx(Aaitax,Monhahare), membyx(Monhahare,Temiarirox).

	teindyo(Teindyx,Teindyo):- membyo(Tuva, Teindyo),
														 monhahare(Tuva, Teindyx),
														 Teindyo <> Teindyx,
														 membyo(Sy, Teindyo),
													 	 monhahare(Sy, Teindyx),
														 Tuva>Sy.

	teindyoGuasuve(Teindyx,Teindyo):- membyo(Tuva, Teindyo),
														 monhahare(Tuva, Teindyx),
														 Teindyo <> Teindyx,
														 membyo(Sy, Teindyo),
														 monhahare(Sy, Teindyx),
														 Tuva>Sy,
													 	 nac(Teindyx,_,Day1,Month1,Year1),
													 	 nac(Teindyo,_,Day2,Month2,Year2),
													 	 omboaraGuasuve(Day1,Month1,Year1,Day2,Month2,Year2).

